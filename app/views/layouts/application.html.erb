<!DOCTYPE html>
<html>
<head>
  <title>Historias Clinicas: <%= yield :title%> <%=@titulo%></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="page">
		<div id="header">
			<div id="logo">
				Historias Clinicas
			</div>
			<div id="menu">
				<ul id="menu_items">
					<li><%=link_to 'Pacientes', pacientes_path%></li>
					<li><%=link_to 'Configuracion', estudios_path%></li>
				</ul>				
			</div>
		</div>
		<div id="workspace">
			<%= content_for?(:content) ? yield(:content) : yield%>	
		</div>
	</div>
</body>
</html>
